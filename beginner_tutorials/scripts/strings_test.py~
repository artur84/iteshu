#!/usr/bin/env python

import rospy
from std_msgs.msg import String

def my_main():
    rospy.init_node('strings_test', anonymous=True)
    pub = rospy.Publisher('reconizer/output', String, queue_size=10)
    rate = rospy.Rate(0.1) # 10hz
    mi_string_1 = 'avanza'
    mi_string_2 = 'back'
    mi_string_3 = 'stop'

    count = 0

    while not rospy.is_shutdown():
        #if mi_string_1 == mi_string_2:
        #    print 'string 1 y 2 son iguales'
        #elif mi_string_1 == mi_string_3:
        #    print 'string 1 y 3 son iguales'
        #elif mi_string_2 == mi_string_3:
        #    print 'string 2 y 3 son iguales'
        #    print mi_string_2
        #    print mi_string_3
		
	
        #hello_str = "hello world %s" % rospy.get_time()
        #rospy.loginfo(hello_str)
        
        if count ==0:
            pub.publish(mi_string_1)
            print mi_string_1
        elif count == 1:
            pub.publish(mi_string_2)
            print mi_string_2
        elif count == 2:
            count=-1
            pub.publish(mi_string_3)
            print mi_string_3
        count+=1
        rate.sleep()

if __name__ == '__main__':
    try:
        my_main()
    except rospy.ROSInterruptException:
        print 'i died'
