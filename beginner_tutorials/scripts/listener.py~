#!/usr/bin/env python
import rospy
from std_msgs.msg import String

def callback(mi_string_que_recibo_en_el_callback):
    print mi_string_que_recibo_en_el_callback
    
    
def listener():

    rospy.init_node('listener', anonymous=True)

    rospy.Subscriber("recognizer/output", String, callback)

    rospy.spin()

if __name__ == '__main__':
    listener()
